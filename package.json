		"electron-store": "^8.1.0",
		"https": "^1.0.0",
		"os": "^0.1.2",
		"path": "^0.12.7",
		"path-browserify": "^1.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.20.0",
		"recoil": "^0.7.7",
		"request": "2.88.2",
		"stream": "^0.0.2",
		"terser": "5.26.0",
		"uuid": "^9.0.1",
		"yt-dlp-wrap": "^2.3.12"
	},
	"devDependencies": {
