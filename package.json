{
  "name": "videofetcher",
  "description": "in dev",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "kurosiko"
  },
  "repository": {
    "url": "https://github.com/kurosiko/electron-project.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon .",
    "test": "vitest"
  },
  "build": {
    "appId": "com.electron.vf",
    "directories": {
      "output": "output"
    },
    "files": [
      "dist/**.js",
      "dist/app.css",
      "dist/index.html",
      "package.json",
      "node_modules"
    ],
    "win": {
      "target": "nsis",
      "icon": "C:\\Users\\kurosiko\\Desktop\\youtubedl.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "electronmon": {
    "patterns": [
      "dist/**/*",
      "!config/**",
      "!resource/**",
      "!thumbnail.*",
      "!temp/**",
      "!*.exe"
    ]
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "child_process": "^1.0.2",
    "https": "^1.0.0",
    "image-size": "^1.1.1",
    "immer": "^10.1.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-slide-routes": "^3.0.3",
    "recoil": "^0.7.7",
    "request": "^2.88.2",
    "stream": "^0.0.2",
    "terser": "5.26.0",
    "uuid": "^9.0.1",
    "ytmusic-api": "^5.2.1"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.26",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/adm-zip": "^0.5.5",
    "@types/image-size": "^0.8.0",
    "@types/node": "^20.10.0",
    "@types/path-browserify": "^1.0.2",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.3.0",
    "@types/request": "^2.48.12",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^30.0.6",
    "electron-builder": "^24.9.1",
    "electronmon": "^2.0.2",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-utf8-bom": "^1.4.0"
  }
}
